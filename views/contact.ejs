<script>
  const currentLocation = location.href;
  console.log("Current location", location.href);

  const navbarLinks = document.querySelectorAll(".navbar-link");

  console.log(navbarLinks);

  for (let i = 0; i < navbarLinks.length; i++) {
    if (navbarLinks[i].href == currentLocation) {
      console.log(navbarLinks[i]);
      navbarLinks[i].className = "navbar-link  active";
    }
  }

  function menuClicked(){

let toggler = document.getElementById('toggler')
let navMenu = document.getElementById('mobileNav')

navMenu.classList.toggle('showmobilenav')
navMenu.classList.toggle('hidemobilenav')
toggler.classList.toggle('active')
toggler.classList.toggle('inactive')
}


window.addEventListener('scroll', function() { 
  let navbar = document.querySelector('.navbar-container') ;

  let currentPosition = window.scrollY ;

  if(currentPosition>100){
    navbar.classList.add('navbar-white')}
    else{
      navbar.classList.remove('navbar-white')
    }

})



</script>

